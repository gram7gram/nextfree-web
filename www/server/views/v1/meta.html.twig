<link rel="stylesheet" href="{{ parameters.storageHost }}/css/v1/bootstrap.css"
      crossorigin="anonymous">
<link rel="stylesheet" href="{{ parameters.storageHost }}/css/v1/style.css"
      crossorigin="anonymous">

{% if parameters.gtm.enabled == true %}
    <script async src="https://www.googletagmanager.com/gtag/js?id={{ parameters.gtm.code }}"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag() {dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', '{{ parameters.gtm.code }}');
    </script>
{% endif %}