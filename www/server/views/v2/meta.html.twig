{% set version = '2020-01-30' %}
<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1">

<meta name="robots" content="noindex, nofollow">
{#<meta name="robots" content="index, follow" />#}

<meta name="application-name" content="NextFree"/>
<link rel="apple-touch-icon-precomposed" sizes="57x57" href="{{ parameters.storageHost }}/apple-touch-icon-57x57.png?v={{ version }}"/>
<link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ parameters.storageHost }}/apple-touch-icon-114x114.png?v={{ version }}"/>
<link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ parameters.storageHost }}/apple-touch-icon-72x72.png?v={{ version }}"/>
<link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ parameters.storageHost }}/apple-touch-icon-144x144.png?v={{ version }}"/>
<link rel="apple-touch-icon-precomposed" sizes="60x60" href="{{ parameters.storageHost }}/apple-touch-icon-60x60.png?v={{ version }}"/>
<link rel="apple-touch-icon-precomposed" sizes="120x120" href="{{ parameters.storageHost }}/apple-touch-icon-120x120.png?v={{ version }}"/>
<link rel="apple-touch-icon-precomposed" sizes="76x76" href="{{ parameters.storageHost }}/apple-touch-icon-76x76.png?v={{ version }}"/>
<link rel="apple-touch-icon-precomposed" sizes="152x152" href="{{ parameters.storageHost }}/apple-touch-icon-152x152.png?v={{ version }}"/>
<link rel="icon" type="image/png" href="{{ parameters.storageHost }}/favicon-196x196.png?v={{ version }}" sizes="196x196"/>
<link rel="icon" type="image/png" href="{{ parameters.storageHost }}/favicon-96x96.png?v={{ version }}" sizes="96x96"/>
<link rel="icon" type="image/png" href="{{ parameters.storageHost }}/favicon-32x32.png?v={{ version }}" sizes="32x32"/>
<link rel="icon" type="image/png" href="{{ parameters.storageHost }}/favicon-16x16.png?v={{ version }}" sizes="16x16"/>
<link rel="icon" type="image/png" href="{{ parameters.storageHost }}/favicon-128.png?v={{ version }}" sizes="128x128"/>
<meta name="msapplication-TileColor" content="#FFFFFF"/>
<meta name="msapplication-TileImage" content="{{ parameters.storageHost }}/mstile-144x144.png?v={{ version }}"/>
<meta name="msapplication-square70x70logo" content="{{ parameters.storageHost }}/mstile-70x70.png?v={{ version }}"/>
<meta name="msapplication-square150x150logo" content="{{ parameters.storageHost }}/mstile-150x150.png?v={{ version }}"/>
<meta name="msapplication-wide310x150logo" content="{{ parameters.storageHost }}/mstile-310x150.png?v={{ version }}"/>
<meta name="msapplication-square310x310logo" content="{{ parameters.storageHost }}/mstile-310x310.png?v={{ version }}"/>

<link rel="stylesheet" href="{{ parameters.storageHost }}/css/v2/bootstrap.css?v={{ version }}"
      crossorigin="anonymous">
<link rel="stylesheet" href="{{ parameters.storageHost }}/css/v2/style.css?v={{ version }}"
      crossorigin="anonymous">

<link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,800|Open+Sans:400,700&display=swap"
      rel="stylesheet">

<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.2/css/all.css"
      integrity="sha384-/rXc/GQVaYpyDdyxK+ecHPVYJSN9bmVFBvjA/9eOB+pb3F2w2N6fc5qB9Ew5yIns"
      crossorigin="anonymous">

{% if parameters.gtm.enabled == true %}
    <script async src="https://www.googletagmanager.com/gtag/js?id={{ parameters.gtm.code }}"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag() {dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', '{{ parameters.gtm.code }}');
    </script>
{% endif %}